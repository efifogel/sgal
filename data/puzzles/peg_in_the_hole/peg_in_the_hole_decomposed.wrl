#VRML V2.0 utf8
NavigationInfo { type "EXAMINE" }
Configuration {
  accumulation Accumulation { enabled TRUE }
  stencilBits 1
}
ColorBackground {
  color 1 1 1 0
  clearStencil TRUE
}
# Viewpoint {
#   type "ORTHOGONAL"
#   fieldOfView 2.95
#   position 0 0 10
# }
Viewpoint {
  type "ORTHOGONAL"
  fieldOfView 3.08
  position 0 0 10
}

DEF DRAW_AOS_KEY SingleKeySensor { key "c" }
DEF DRAW_AOS_HALOED_KEY SingleKeySensor { key "h" state TRUE }
DEF DRAW_ALT_KEY SingleKeySensor { key "m" }
DEF INC_ALT_KEY SingleKeySensor { key "i" }
DEF DRAW_AOS_MINK_SUM_KEY SingleKeySensor { key "a" }
DEF INC_MINK_SUM_KEY SingleKeySensor { key "d" }
DEF SOLVE_KEY SingleKeySensor { key "s" }

DEF AOS_PIECE_RED_KEY SingleKeySensor { key "r" }
DEF AOS_PIECE_BLUE_KEY SingleKeySensor { key "b" }

DEF AOS_PIECE_0_KEY SingleKeySensor { key "u" }
DEF AOS_PIECE_1_KEY SingleKeySensor { key "v" }
DEF AOS_PIECE_2_KEY SingleKeySensor { key "w" }

DEF SNAP_KEY SingleKeySensor { key "S" }
DEF SNAP_R_KEY SingleKeySensor { key "R" }
DEF SNAP_B_KEY SingleKeySensor { key "B" }

DEF SNAP Snapshot {
  image Image { }
  fileFormat "jpg"
  quality 98
  sequence FALSE
  dirName "."
  fileName "peg_in_the_hole"
}

# Singles:
DEF SNAP_R Snapshot {
  image Image { }
  fileFormat "jpg"
  quality 98
  sequence FALSE
  dirName "."
#  fileName "red"
  fileName "reflected-red"
}

DEF SNAP_B Snapshot {
  image Image { }
  fileFormat "jpg"
  quality 98
  sequence FALSE
  dirName "."
#  fileName "blue"
  fileName "reflected-blue"
}

Transform {
  bboxCenter 0 0 0
  bboxSize 1.5 1.5 1.5
  rotation 1 0 0 .5235
  children [
    Transform {
      bboxCenter 0 0 0
      bboxSize 1.5 1.5 1.5
      # #rotation 0 -1 0 .5235 # 30 deg
      rotation 0 -1 0 3.6651 # 210 deg
      children [
	Transform {
	  bboxCenter 0 0 0
	  bboxSize 1.5 1.5 1.5
	  rotation -1 0 0 1.5707
	  children [
	    # The axes
	    Switch {
	      whichChoice 0
	      children [
		Transform {
#		  scale 0.3 0.3 0.3
		  scale 0.7 0.7 0.7
		  children [
		    Transform {
		      translation 2.5 0 0
		      rotation 1 0 0 1.5708
		      children [
			Transform {
			  translation 0 -0.3 0
			  children [
			    Shape {
			      priority 10
			      appearance DEF TEXT_APP Appearance {
				material Material { diffuseColor 0 0 0 }
			      }
			      geometry Text {
				string "X"
				fontStyle FontStyle { size 0.5 }
			      }
			    }
			   ]
			}
		       ]
		    }
		    Transform {
		      translation 0 2.5 0
		      rotation 1 0 0 1.5708
		      children [
			Transform {
			  translation -0.2 0 0
			  children [
			    Shape {
			      priority 10
			      appearance USE TEXT_APP
			      geometry Text {
				string "Y"
				fontStyle FontStyle { size 0.5 }
			      }
			    }
			   ]
			}
		       ]
		    }
		    Transform {
		      translation 0 0 2.5
		      rotation 1 0 0 1.5708
		      children [
			Transform {
			  translation -0.2 0 0
			  children [
			    Shape {
			      priority 10
			      appearance USE TEXT_APP
			      geometry Text {
				string "Z"
				fontStyle FontStyle { size 0.5 }
			      }
			    }
			   ]
			}
		       ]
		    }
		    Shape {
		      appearance DEF CS_APP Appearance {
			material Material { diffuseColor 0.5 0.5 0.5 }
		      }
		      geometry Sphere { radius 0.1 }
		    }
		    DEF CS_AXIS Transform {
		      translation 0 1 0
		      children [
			Shape {
			  appearance USE CS_APP
			  geometry DEF CS_CYLINDER Cylinder {
			    radius 0.1
			    set_is_bottom_visible FALSE
			    set_is_top_visible FALSE
			  }
			}
			Transform {
			  translation 0 1.2 0
			  children [
			    Shape {
			      appearance USE CS_APP
			      geometry DEF CS_CONE Cone {
				bottomRadius 0.2
				height 0.4
			      }
			    }
			   ]
			}
		       ]
		    }
		    Transform {
		      rotation 0 0 1 -1.57
		      children [ USE CS_AXIS ]
		    }
		    Transform {
		      rotation 1 0 0 1.57
		      children [ USE CS_AXIS ]
		    }
		   ]
		}
	       ]
	    }
	    DEF PUZZLE Assembly {
	      children [
		# Piece red:
		DEF SWITCH_RED Switch {
		  whichChoice 0
		  children [
		    DEF PIECE_RED AssemblyPart {
		      children [
			DEF SWITCH_RED_0 Switch {
			  whichChoice 0
			  children [
			    Shape {
			      priority 6
			      appearance DEF APP_RED Appearance {
				material Material {
				  diffuseColor 1 0 0
				  transparency 0.5
				}
			      }
			      geometry DEF PIECE_RED_0 SphericalGaussianMapColored {
				solid TRUE
				drawAosSphere TRUE
				drawAosHaloed TRUE
				aosVertexStyle "none"
				aosEdgeColor 1 0 0
				coord DEF COORD_RED ExactCoordinate {
				  exactPoint\[-1 -4 -1,
					    1 -4 -1,
					    1  0 -1,
					   -1  0 -1,
					   -1 -4  1,
					    1 -4  1,
					    1  0  1,
					   -1  0  1
					  ]
				}
				coordIndex[3 2 1 0 -1,
					   4 5 6 7 -1,
					   0 1 5 4 -1,
					   7 6 2 3 -1,
					   4 7 3 0 -1,
					   1 2 6 5 -1
					  ]
			      }
			    }
			   ]
			}
		       ]
		    }
		   ]
		}

		# Piece blue:
		DEF SWITCH_BLUE Switch {
		  whichChoice 0
		  children [
		    DEF PIECE_BLUE AssemblyPart {
		      children [
			DEF SWITCH_BLUE_0 Switch {
			  whichChoice -1
			  children [
			    Shape {
			      priority 1
			      appearance DEF APP_BLUE Appearance {
				material Material {
				  diffuseColor 0 0 1
				  transparency 0.5
				}
			      }
			      geometry DEF PIECE_BLUE_0 SphericalGaussianMapColored {
				drawAosSphere TRUE
				drawAosHaloed TRUE
				aosVertexStyle "none"
				aosEdgeColor 0 0 1
				coord ExactCoordinate {
				  exactPoint\[-2  0 -2,
					    2  0 -2,
					    2  2 -2,
					   -2  2 -2,
					   -2  0  2,
					    2  0  2,
					    2  2  2,
					   -2  2  2
					  ]
				}
				coordIndex[3 2 1 0 -1,
					   4 5 6 7 -1,
					   0 1 5 4 -1,
					   7 6 2 3 -1,
					   4 7 3 0 -1,
					   1 2 6 5 -1
					  ]
			      }
			    }
			   ]
			}
			DEF SWITCH_BLUE_1 Switch {
			  whichChoice 0
			  children [
			    Shape {
			      priority 2
			      appearance USE APP_BLUE
			      geometry DEF PIECE_BLUE_1 SphericalGaussianMapColored {
				drawAosSphere TRUE
				drawAosHaloed TRUE
				aosVertexStyle "none"
				aosEdgeColor 0 0 1
				coord ExactCoordinate {
				  exactPoint\[-2 -2 -2,
					   -1 -2 -2,
					   -1  0 -2,
					   -2  0 -2,
					   -2 -2  2,
					   -1 -2  2,
					   -1  0  2,
					   -2  0  2
					  ]
				}
				coordIndex[3 2 1 0 -1,
					   4 5 6 7 -1,
					   0 1 5 4 -1,
					   7 6 2 3 -1,
					   4 7 3 0 -1,
					   1 2 6 5 -1
					  ]
			      }
			    }
			   ]
			}
			DEF SWITCH_BLUE_2 Switch {
			  whichChoice 0
			  children [
			    Shape {
			      priority 5
			      appearance USE APP_BLUE
			      geometry DEF PIECE_BLUE_2 SphericalGaussianMapColored {
				drawAosSphere TRUE
				drawAosHaloed TRUE
				aosVertexStyle "none"
				aosEdgeColor 0 0 1
				coord ExactCoordinate {
				  exactPoint\[ 1 -2 -2,
					    2 -2 -2,
					    2  0 -2,
					    1  0 -2,
					    1 -2  2,
					    2 -2  2,
					    2  0  2,
					    1  0  2
					  ]
				}
				coordIndex[3 2 1 0 -1,
					   4 5 6 7 -1,
					   0 1 5 4 -1,
					   7 6 2 3 -1,
					   4 7 3 0 -1,
					   1 2 6 5 -1
					  ]
			      }
			    }
			   ]
			}
			DEF SWITCH_BLUE_3 Switch {
			  whichChoice 0
			  children [
			    Shape {
			      priority 3
			      appearance USE APP_BLUE
			      geometry DEF PIECE_BLUE_3 SphericalGaussianMapColored {
				drawAosSphere TRUE
				drawAosHaloed TRUE
				aosVertexStyle "none"
				aosEdgeColor 0 0 1
				coord ExactCoordinate {
				  exactPoint\[-1 -2 -2,
					    1 -2 -2,
					    1  0 -2,
					   -1  0 -2,
					   -1 -2 -1,
					    1 -2 -1,
					    1  0 -1,
					   -1  0 -1
					  ]
				}
				coordIndex[3 2 1 0 -1,
					   4 5 6 7 -1,
					   0 1 5 4 -1,
					   7 6 2 3 -1,
					   4 7 3 0 -1,
					   1 2 6 5 -1
					  ]
			      }
			    }
			   ]
			}
			DEF SWITCH_BLUE_4 Switch {
			  whichChoice 0
			  children [
			    Shape {
			      priority 4
			      appearance USE APP_BLUE
			      geometry DEF PIECE_BLUE_4 SphericalGaussianMapColored {
				drawAosSphere TRUE
				drawAosHaloed TRUE
				aosVertexStyle "none"
				aosEdgeColor 0 0 1
				coord ExactCoordinate {
				  exactPoint\[-1 -2 1,
					    1 -2 1,
					    1  0 1,
					   -1  0 1,
					   -1 -2 2,
					    1 -2 2,
					    1  0 2,
					   -1  0 2
					  ]
				}
				coordIndex[3 2 1 0 -1,
					   4 5 6 7 -1,
					   0 1 5 4 -1,
					   7 6 2 3 -1,
					   4 7 3 0 -1,
					   1 2 6 5 -1
					  ]
			      }
			    }
			   ]
			}

		       ]
		    }
		   ]
		}
	       ]
	    }
	   ]
	}
       ]
    }
   ]
}

# Piece red
DEF INC_RED Incrementor {
  minValue -1
  maxValue 0
  value 0
}

ROUTE AOS_PIECE_RED_KEY.state TO INC_RED.trigger
ROUTE INC_RED.value TO SWITCH_RED.whichChoice

# Piece blue:
DEF INC_BLUE Incrementor {
  minValue -1
  maxValue 0
  value 0
}

ROUTE AOS_PIECE_BLUE_KEY.state TO INC_BLUE.trigger
ROUTE INC_BLUE.value TO SWITCH_BLUE.whichChoice

# Sub-part 0
DEF INC_0 Incrementor {
  minValue -1
  maxValue 0
  value 0
}

ROUTE AOS_PIECE_0_KEY.state TO INC_0.trigger
ROUTE INC_0.value TO SWITCH_RED_0.whichChoice
ROUTE INC_0.value TO SWITCH_BLUE_0.whichChoice

# Sub-part 1
DEF INC_1 Incrementor {
  minValue -1
  maxValue 0
  value 0
}

ROUTE AOS_PIECE_1_KEY.state TO INC_1.trigger
ROUTE INC_1.value TO SWITCH_BLUE_1.whichChoice

# Sub-part 2
DEF INC_2 Incrementor {
  minValue -1
  maxValue 0
  value 0
}

ROUTE AOS_PIECE_2_KEY.state TO INC_2.trigger
ROUTE INC_2.value TO SWITCH_BLUE_2.whichChoice

#

ROUTE DRAW_AOS_KEY.state TO PIECE_RED_0.drawAos

ROUTE DRAW_AOS_KEY.state TO PIECE_BLUE_0.drawAos
ROUTE DRAW_AOS_KEY.state TO PIECE_BLUE_1.drawAos
ROUTE DRAW_AOS_KEY.state TO PIECE_BLUE_2.drawAos
ROUTE DRAW_AOS_KEY.state TO PIECE_BLUE_3.drawAos
ROUTE DRAW_AOS_KEY.state TO PIECE_BLUE_4.drawAos

ROUTE SNAP_KEY.state TO SNAP.trigger
ROUTE SNAP_R_KEY.state TO SNAP_R.trigger
ROUTE SNAP_B_KEY.state TO SNAP_B.trigger

ROUTE SOLVE_KEY.state TO PUZZLE.trigger

ROUTE DRAW_ALT_KEY.state TO PUZZLE.drawAlternate
ROUTE INC_ALT_KEY.state TO PUZZLE.incAlternate
ROUTE DRAW_AOS_MINK_SUM_KEY.state TO PUZZLE.drawAosMinkowskiSums
ROUTE INC_MINK_SUM_KEY.state TO PUZZLE.incMinkowskiSums
